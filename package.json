{
  "name": "service-name",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "pnpm clear && pnpm i && cross-env ANALYZE=true APP_ENV=production NODE_OPTIONS=--max_old_space_size=4096 PORT=8080 next build",
    "dev": "cross-env NODE_ENV=development next dev -p 3000",
    "build:dev": "cross-env NODE_ENV=dev next build",
    "build:staging": "cross-env NODE_ENV=test next build",
    "build": "cross-env BUILD_MODE=$BUILD_MODE node set-node-env.js",
    "docker:build": "docker build -t service-name .",
    "docker:run": "docker run -p 3000:3000 service-name",
    "start": "next start -p 3002",
    "clear": "rm -rf .next && rm -rf node_modules",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{js,ts,tsx,css}\"",
    "create-lang": "plop"
  },
  "dependencies": {
    "@connectrpc/connect": "1.6.1",
    "@connectrpc/connect-node": "1.6.1",
    "@connectrpc/connect-web": "1.6.1",
    "@hookform/resolvers": "2.9.11",
    "@tailwindcss/container-queries": "0.1.1",
    "autoprefixer": "10.4.20",
    "axios": "0.27.2",
    "clsx": "1.2.1",
    "cookies-next": "4.3.0",
    "cross-env": "7.0.3",
    "dayjs": "1.11.13",
    "google-protobuf": "3.21.4",
    "jotai": "2.8.0",
    "next": "14.1.1",
    "next-intl": "3.25.3",
    "next-seo": "4.29.0",
    "next-themes": "0.2.1",
    "postcss": "8.4.49",
    "query-string": "8.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.53.2",
    "react-modal": "3.16.1",
    "react-query": "3.39.3",
    "react-select": "5.8.3",
    "sass": "1.81.0",
    "sharp": "0.33.5",
    "sonner": "0.6.2",
    "tailwind-merge": "1.14.0",
    "tailwind-variants": "0.1.20",
    "tailwindcss": "3.4.15",
    "tailwindcss-animate": "1.0.7",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.7",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.17.10",
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "@types/react-modal": "3.16.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-tailwind": "0.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "next-transpile-modules": "10.0.1",
    "plop": "4.0.1",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.5.14",
    "typescript": "5.7.2"
  },
  "browser": {
    "fs": false,
    "request": false,
    "@connectrpc/connect-node": false
  }
}
